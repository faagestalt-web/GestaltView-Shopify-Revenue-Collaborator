# Copy this file to `.env` and replace the placeholder values with your actual credentials.

SHOPIFY_API_KEY=your_shopify_api_key
SHOPIFY_API_SECRET=your_shopify_api_secret
APP_URL=https://your-ngrok-or-cloud-url
SCOPES=read_products,write_products,read_orders

# Optional: API keys for AI services (OpenAI, Anthropic, etc.)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Optional: Database connection string for Manifest Index storage
DATABASE_URL=postgresql://localhost/gestaltview
